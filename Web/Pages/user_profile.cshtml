@page "/user_profile"
@addTagHelper *,Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Crypto Devils!</title>
    <base href="/" />
    <link rel="stylesheet" type="text/css" href="CSS/header.css">
    <link rel="stylesheet" type="text/css" href="CSS/footer.css">
    <link rel="stylesheet" type="text/css" href="CSS/analytics.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
</head>
<body>
    <component type="typeof(Capstone1.App)" render-mode="ServerPrerendered" />
    <script src="_framework/blazor.server.js"></script>
    @*<component type="typeof(Capstone1.Shared.MainLayout)" render-mode="ServerPrerendered" />*@

    <div class="row-one">
        <div class="logo">
            <a href="dashboard.html"><img src="Images/CryptoDevil.jpg" alt="CryptoDevil"></a>

            <div class="social">
                <a href="https://www.instagram.com/">
                    <img src="Images/instagram.png" alt="Cryptobot Instagram">
                </a>
                <a href="https://www.twitter.com/">
                    <img src="Images/twitter.png" alt="Cryptobot Twitter">
                </a>
            </div>
        </div>
    </div>

    <div class="row-one">
        <div class="column-one">
            <div class="list">
                <ul>
                    <li><a href="dashboard">Dashboard</a></li>
                    <li><a href="exchange_history">Exchange History</a></li>
                    <li><a href="analytics">Analytics</a></li>
                    <li><a href="statistics">Statistics</a></li>
                    <li><a href="alerts">Alerts</a></li>
                </ul>
            </div>
        </div>
        <div class="column-two">
            <div class="style_h1">
                <h1>My Account</h1>
            </div>
            <div class="account_photo">
                <img src="Images/default_user.png" alt="User Photo">
                <h2>
                    First Last Name
                </h2>
                <p>
                    Email
                </p>
                <p>
                    Phone Number
                </p>
                <p>
                    Street Address
                </p>
            </div>
            <br>
            <br>
            <br>

            <div class="subscription">
                <h1>Subscription Information</h1>
                <p>
                    Subscription Type
                </p>
                <p>
                    Subscription Anniversary Date
                </p>
                <p>
                    Subscription Fee Dues
                </p>
                <p>
                    Subscription Expiration
                </p>
            </div>

            <br>
            <br>
            <div class="footer">
                <ul>
                    <li><a href="about.html">Ownership Info</a></li>
                    <li><a href="contact.html">Contact Info</a></li>
                </ul>
            </div>
        </div>
    </div>

    <div class="profile_button">
        <a href="user_profile"><img src="Images/default_user.png" alt="User Photo"></a>
    </div>

</body>
</html>